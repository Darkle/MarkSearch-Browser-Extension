{
  "name": "marksearch-browser-extension",
  "version": "2017.1.23",
  "description": "MarkSearch Browser Extension",
  "scripts": {
    "chrome": "cross-env browser=chrome webpack --progress --colors --watch",
    "firefox": "cross-env browser=firefox webpack --progress --colors --watch",
    "edge": "cross-env browser=edge webpack --progress --colors --watch",
    "analyseBundle": "cross-env browser=chrome runBundleAnalyzer=true webpack --progress --colors --watch",
    "analyseMinifiedBundle": "cross-env browser=chrome NODE_ENV=production runBundleAnalyzer=true webpack --progress --colors",
    "buildChrome": "cross-env browser=chrome NODE_ENV=production webpack --progress --colors ",
    "buildFirefox": "cross-env browser=firefox NODE_ENV=production webpack --progress --colors ",
    "buildEdge": "cross-env browser=edge NODE_ENV=production webpack --progress --colors ",
    "generateStats": "cross-env browser=chrome webpack --profile --json > stats.json",
    "serve": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Darkle/MarkSearch-Browser-Extension.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Darkle/MarkSearch-Browser-Extension/issues"
  },
  "homepage": "https://github.com/Darkle/MarkSearch-Browser-Extension#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-babili": "0.0.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.11.1",
    "babili-webpack-plugin": "0.0.7",
    "bell-on-bundler-error-plugin": "^1.0.8",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "dompurify": "^0.8.4",
    "eslint": "^3.14.1",
    "eslint-loader": "^1.6.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "^0.5.4",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "stem-porter": "0.0.1",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "url-loader": "^0.5.7",
