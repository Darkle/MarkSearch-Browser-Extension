    "webpack-bundle-analyzer": "^2.2.0",
    "webpack-cleanup-plugin": "^0.4.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-load-plugins": "^0.1.2",
    "webpack-merge": "^2.4.0",
    "write-json-webpack-plugin": "^1.0.0"
  },
